#: sources.list.5.xml
#, no-wrap
msgid ""
"Types: deb\n"
"URIs: file:/home/apt/debian\n"
"Suites: unstable\n"
"Components: main contrib non-free"
msgstr ""
"Types: deb\n"
"URIs: file:/home/apt/debian\n"
"Suites: unstable\n"
"Components: main contrib non-free"

