#: apt-cache.8.xml:390 apt-cdrom.8.xml:163 apt-config.8.xml:111 apt-extracttemplates.1.xml:81 apt-ftparchive.1.xml:625 apt-get.8.xml:571 apt-mark.8.xml:135 apt-sortpkgs.1.xml:76
