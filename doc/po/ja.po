#: apt_preferences.5.xml:16
msgid ""
"&apt-author.team; &apt-email; &apt-product; <date>16 February 2010</date>"
msgstr ""
"&apt-author.team; &apt-email; &apt-product; <date>16 February 2010</date>"

# type: Content of: <refentry><refnamediv><refname>
#. type: Content of: <refentry><refnamediv><refname>
#: apt_preferences.5.xml:24 apt_preferences.5.xml:31
